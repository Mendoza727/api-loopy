{
  "name": "api-loopy",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "start": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
    "run-migrations": "npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/config/database.ts",
    "generate-migration": "npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -n",
    "revert-migrations": "npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert -d src/config/database.ts",
    "show-migrations": "npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:show -d src/config/database.ts",
    "create-docker-container": "docker-compose up --build",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "canvas": "^3.0.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "_moduleAliases": {
    "@": "./dist"
  }
}
